sameSize, grid, justify
LayLa.Insert
(CONFIG
	(DEF Grid (CONFIG
		(DEF vgrid STATIC) (DEF hgrid STATIC) (DEF sameWidth FALSE) (DEF sameHeight FALSE)
		(TABLE Panel (rows=3 cols=3 w=181 h=181 border=8 orientation=HOR hgrid = hgrid vgrid=vgrid
								sameWidth=sameWidth sameHeight=sameHeight hjustify=RIGHT vjustify=TOP)
			(NEW Button (w=[1] h=[1]) (ATTR Caption = "1"))
			(NEW Button (w=20 h=20) (ATTR Caption = "2"))
			(NEW Button (w=[1] h=[1]) (ATTR Caption = "3"))
			(NEW Button (w=20 h=20 hjustifyMe=LEFT) (ATTR Caption = "4"))
			(NEW Button (w=30 h=30 hjustifyMe=CENTER vjustifyMe=CENTER) (ATTR Caption = "5"))
			(NEW Button (w=20 h=20 vjustifyMe=BOTTOM) (ATTR Caption = "6"))
			(NEW Button (w=[1] h=[1]) (ATTR Caption = "7"))
			(NEW Button (w=20 h=20 hjustifyMe=LEFT vjustifyMe=BOTTOM) (ATTR Caption = "8"))
			(NEW Button (w=[1] h=[1]) (ATTR Caption = "9"))
		)))
	(TABLE Panel (cols=3 border=8)
		VIRTUAL
		(NEW Grid)
		VIRTUAL
		(NEW Grid ( hgrid=DYNAMIC))
		(NEW Grid ( vgrid=DYNAMIC))
		(NEW Grid (PARAMS hgrid=DYNAMIC vgrid=DYNAMIC))
		(NEW Grid ( hgrid=DYNAMIC vgrid=DYNAMIC sameWidth=TRUE))
		(NEW Grid (PARAMS hgrid=DYNAMIC vgrid=DYNAMIC sameHeight=TRUE))
		(NEW Grid (PARAMS hgrid=DYNAMIC vgrid=DYNAMIC sameWidth=TRUE sameHeight=TRUE))
))


complex positioning
LayLa.Insert
(TABLE Panel (border=8 cols=3 rows=3 orientation=VERT w=151 h=151)
	(NEW Button (w=[1] h=[1]) (ATTR Caption = "1"))
	(HLIST VIRTUAL (w=[1] h=[1]) (NEW Button (w=20 h=20 hjustifyMe=RIGHT) (ATTR Caption = "2")))
	(NEW Button (w=[1] h=[1]) (ATTR Caption = "3"))
	(HLIST VIRTUAL (w=[1] h=[1]) (NEW Button (w=20 h=20) (ATTR Caption = "4")))
	(NEW Button (w=30 h=30 hjustifyMe = RIGHT vjustifyMe=TOP) (ATTR Caption = "5"))
	(HLIST VIRTUAL (w=[1] h=[1]) (NEW Button (w=20 h=20) (ATTR Caption = "6")))
	(NEW Button (w=[1] h=[1]) (ATTR Caption = "7"))
	(HLIST VIRTUAL (w=[] h=[]) (NEW Button (w=20 h=20 vjustifyMe=TOP) (ATTR Caption = "8")))
	(NEW Button (w=[] h=[] hjustifyMe=RIGHT vjustifyMe=TOP) (ATTR Caption = "9")))


Desktop.Panel
LayLa.Insert
(HLIST Panel (sameSize=TRUE dist=10 border=8)
	(NEW List
		(ATTR (SET Name "List") (SET Cmd "Desktops.OpenDoc '#Point '")))

	(VLIST VIRTUAL (dist=2)
		(NEW List (w=[1] h=100)
			(ATTR (SET Name "List1") (SET Cmd "Lists.Directory '#Point ' List")))

		(TABLE VIRTUAL (sameSize=TRUE dist=2 cols=3)
			(NEW Button (h=20)
				(ATTR Caption="Show" Cmd="Lists.Directory '&Show.Value ' List"))
			(SPAN 1 2 (NEW TextField (h=20)
				(ATTR Name="Show" Cmd="Lists.Directory '#Value ' List")))

			(BREAK (NEW VIRTUAL (h=10)))

			(NEW Button (h=20)
				(ATTR Caption="Open" Cmd="Desktops.OpenDoc &Open.Value "))
			(SPAN 1 2 (NEW TextField (h=20)
				(ATTR Name="Open" Cmd="Desktops.OpenDoc #Value ")))	

			(NEW Button (h=20)
				(ATTR (SET Caption "Open ^") (SET Cmd "Desktops.OpenDoc ^")))
			(SPAN 2 1 (NEW Button
				(ATTR (SET Caption "Panel") (SET Cmd "Desktops.OpenDoc (PanelDoc)"))))
			(NEW Button (h=20)
				(ATTR (SET Caption "Text") (SET Cmd "Desktops.OpenDoc (TextDoc)")))

			(NEW Button (h=20)
				(ATTR (SET Caption "Recall") (SET Cmd "Desktops.Recall")))
			(NEW Button (h=20)
				(ATTR (SET Caption "Icon *") (SET Cmd "Icons.InsertIcon *")))
				
			(NEW Button (h=20)
				(ATTR Caption="Delete" Cmd="System.DeleteFiles &Delete.Value  ~"))
			(SPAN 1 2 (NEW TextField (h=20)
				(ATTR Name="Delete")))
	))
)
