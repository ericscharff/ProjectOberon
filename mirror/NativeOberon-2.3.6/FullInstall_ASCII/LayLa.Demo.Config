TABLE with SPAN and BREAK
LayLa.Insert
(TABLE Panel (border=8 cols=3 rows=3)
	(ATTR Locked=TRUE)
	(BREAK (HLIST Panel (border=8) (NEW Caption (ATTR Value="Zwischentitel 0"))))
	(BREAK (HLIST Panel (border=8) (NEW Caption (ATTR Value="Zwischentitel 1"))))
	(NEW Button (ATTR Caption = "1"))
	(SPAN 2 1 (NEW Button (w=[1] h=[1]) (ATTR Caption = "2")))
	(NEW Button  (ATTR Caption = "3"))
	(NEW Button  (ATTR Caption = "4"))
	(NEW Button  (ATTR Caption = "5"))
	(NEW Button  (ATTR Caption = "6"))
	(NEW Button  (ATTR Caption = "7"))
	(BREAK (HLIST Panel (h=60 w=[1] border=8) (HLIST VIRTUAL (vjustifyMe=TOP)
		(NEW Caption (vjustifyMe=CENTER) (ATTR Value="Zwischentitel 2"))
		(NEW Button (hjustifyMe=RIGHT) (ATTR Caption="hello")))
	))	
	(NEW Button  (ATTR Caption = "8"))	
	(NEW Button  (ATTR Caption = "9"))
	(BREAK (HLIST Panel (border=8) (NEW Caption (ATTR Value="Zwischentitel 3"))))	
)))


CONFIG, INCLUDE, same Model in 4 Gadgets
LayLa.Insert
(CONFIG
	(DEF int1 (NEW Integer (ATTR Value=75)))
	(VLIST Panel (border=8)
		(VLIST VIRTUAL (border=8)
			(NEW Slider (w = 100 h = 20) (LINKS Model=int1))
			(NEW TextField (w = 100 h = 20) (LINKS Model=int1)))
		(INCLUDE LayLa.Include.Config (PARAMS (SET int int1) (SET MyBorder 8)))
		(INCLUDE LayLa.Include.Config (MyBorder=3))
		(INCLUDE LayLa.Include.Config (MyBorder=8))
	)
)


